{
  "name": "@alexandernanberg/rapier-2d",
  "version": "0.19.3",
  "description": "2-dimensional physics engine - official JS bindings",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/rapier.d.mts",
      "default": "./dist/rapier.mjs"
    },
    "./compat": {
      "types": "./dist/compat.d.mts",
      "default": "./dist/compat.mjs"
    },
    "./simd": {
      "types": "./dist/simd.d.mts",
      "default": "./dist/simd.mjs"
    },
    "./compat-simd": {
      "types": "./dist/compat-simd.d.mts",
      "default": "./dist/compat-simd.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "physics",
    "dynamics",
    "rigid",
    "real-time",
    "joints",
    "rapier",
    "2d"
  ],
  "author": "SÃ©bastien Crozet <developer@crozet.re>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dimforge/rapier.js"
  },
  "homepage": "https://rapier.rs",
  "dependencies": {
    "base64-js": "^1.5.1"
  },
  "devDependencies": {
    "tsdown": "^0.20.1",
    "typescript": "^5.7.3"
  }
}
